<?xml version="1.0"?>
<opencv_storage>
<Settings>
  <!-- Number of inner corners per row and column.-->
  <BoardSize_Width> 10</BoardSize_Width>
  <BoardSize_Height>7</BoardSize_Height>

  <!-- The size of a square in metres-->
  <Square_Size>0.022</Square_Size>

  <!-- The input image directory -->
  <Input_Directory>"../data"</Input_Directory>
  <!-- The input image extension (.jpg, .png, etc.) -->
  <Input_Extension>".MOV"</Input_Extension>

</Settings>
</opencv_storage>
